public class BTest {
    static private int[] table;
    private int nElem;
    
    public BTest(int maxSize){
        table = new int[maxSize];
        nElem=0;
    }
    public void add(int value){
        if(nElem>= table.length){
            int [] locTable = new int[table.length*2];
            for(int i =0; i<table.length;i++)
                locTable[i]=table[i];
                table=locTable;
            
        }
        int j;
        for(j=0; j<nElem;j++)
        if(table[j]> value)break;
        
        for(int k=nElem;k>j;k--)
            table[k]= table[k-1];
                
          
        table[j]=value;
        nElem++;
       
    }
    
    public int get(int index){
        return table[index];
    }
    public int size(){
        return nElem;
    }
    
    
    public boolean remove(int index){
        if(nElem==0 || index>=nElem || index<0)
        return false;
        
        for(int j =index; j<nElem-1; j++)
        {
            table[j]=table[j+1];
        }
        nElem--;
        return true;
    }
    
    public int find(int searchElem){
       int left=0;
       int right=nElem-1;
       int currIndex;
       
       while(true){
           currIndex=(left + right)/2;
           if(table[currIndex]==searchElem) return currIndex;
           else
               if(left>right) return-1;
               else{
                   if(table[currIndex]<searchElem){
                       left = currIndex+1;
                   }
                   else{
                       right=currIndex-1;
                   }
                           
               }
           
       }
    }
    
    public void print(){
        for(int i=0; i<nElem;i++){
            System.out.println(get(i) + " ");
        }
        System.out.println();
    }
    
    
    
    public static void main(String[] args){
        int maxSize =2;
        
        BTest liczba= new BTest(maxSize);
        liczba.add(12);
        liczba.add(92);
        liczba.add(42);
        
        liczba.print();
        
        liczba.remove(1);
        liczba.print();
        liczba.remove(1);
        liczba.print();
        
        liczba.add(23);
        liczba.add(14);
        
        int elem = liczba.find(92);
        System.out.println(elem);
       
        
    }
    
}
